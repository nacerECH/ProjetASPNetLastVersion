
@{
    ViewBag.Title = "Statistic";
    Layout = "~/Views/Shared/_LayoutPageDashboard1.cshtml";
}

<h2>Statistic</h2>


<img src="@Url.Action("MyChart")" />





<div class="col-sm-12">
    <h5 class="mt-4">Basic Pills</h5>
    <hr>

    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <button class="btn btn-outline-secondary" type="button">Button</button>
        </div>
        <select class="custom-select" id="inputGroupSelect03" aria-label="Example select with button addon">
            <option selected>Select Vendeur</option>
            <option value="1"> TUMarket</option>
            <option value="2">Bill Gates Store</option>
            <option value="3">KTSMarket</option>
        </select>
    </div>

    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true">Vues/jours</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#pills-profile" role="tab" aria-controls="pills-profile" aria-selected="false">Vues/Category</a>
        </li>
    
    </ul>
    <div class="tab-content" id="pills-tabContent">
        <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">
            <img src="@Url.Action("MyChart")" />
        </div>
        <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab">
            <img src="@Url.Action("MyChart")" />
        </div>

    </div>
</div>