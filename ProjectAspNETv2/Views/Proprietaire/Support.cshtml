
@{
    ViewBag.Title = "Support";
    Layout = "~/Views/Shared/_LayoutPageDashboard2.cshtml";
}


<h2>Support</h2>




@using (Html.BeginForm("SendMessageToSupport", "Proprietaire", FormMethod.Post))
{

<div class="form-group">
    <label> Email </label>

    
    @Html.TextBox("email", null, new { type = "email", @class="form-control", placeholder="Email" })
</div>

    <div class="form-group">
        <label>Subject</label>
        

        @Html.TextBox("subject", null, new { type = "text", @class = "form-control", placeholder = "Sublect" })
    </div>

    <div class="form-group">
        <label for="message">Message</label>
        @Html.TextBox("message", null,new { @class="form-control",  @rows="3"  })
    </div>




    <input class="label theme-bg2 text-white f-12" type="submit" value="Envoyer" />


}



